{
  "manifest_version": 2,

  "name": "Github Trending New Tab",
  "author": "Anmol Mago",
  "description": "This unofficial extention shows the top trending github repos as provided by http://github.com/trending",
  "incognito": "split",
  "version": "1.2",
    
  "icons": { 
      "16": "assets/images/icon16.png",
      "48": "assets/images/icon48.png",
      "128": "assets/images/icon128.png"
  },

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
    
  "permissions": [
        "storage",
        "http://*.anmolmago.com/"
    ]
}
